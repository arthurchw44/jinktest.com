{
  "name": "dink-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:vite ", 
    "dev:vite": "vite",
    "dev:types": "run-p typecheck:app:watch typecheck:node:watch",
    "typecheck": "run-s typecheck:app typecheck:node",
    "typecheck:app": "npx tsc -p tsconfig.app.json --noEmit",
    "typecheck:node": "npx tsc -p tsconfig.node.json --noEmit",
    "typecheck:watch": "run-p typecheck:app:watch typecheck:node:watch",
    "typecheck:app:watch": "npx tsc -p tsconfig.app.json --noEmit --watch",
    "typecheck:node:watch": "npx tsc -p tsconfig.node.json --noEmit --watch",
    "build": "npx tsc -p tsconfig.app.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.84.0",
    "@tanstack/react-query-devtools": "^5.84.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.11.0",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.10.2"
  }
}
